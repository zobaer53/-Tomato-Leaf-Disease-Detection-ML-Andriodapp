<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zobaer53.plantapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Leaf disease prediction"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Plantapp">
        <activity
            android:name="com.zobaer53.tomato_leaf_disease_detection_ml_androidapp.WebView"
            android:exported="false" />
        <activity
            android:name="com.zobaer53.tomato_leaf_disease_detection_ml_androidapp.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.zobaer53.tomato_leaf_disease_detection_ml_androidapp.splashScreen"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>